    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^7.1.1",
    "@types/yup": "^0.26.22",
    "eslint": "^6.0.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux-starter-kit": "^0.6.1",
