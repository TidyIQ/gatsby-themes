    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@tidyiq/components": "*",
    "@tidyiq/eslint-config": "^2.0.4",
    "@tidyiq/gatsby-theme-tidyiq": "*",
    "eslint": "^6.0.1",
    "gatsby": "^2.13.11",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3"
  }
