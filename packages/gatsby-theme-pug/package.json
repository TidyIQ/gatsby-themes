{
  "name": "@tidyiq/gatsby-theme-pug",
  "version": "0.0.1",
  "description": "Pug theme",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TidyIQ/gatsby-themes.git"
  },
  "keywords": [
    "gatsby"
  ],
  "author": "Jake Clayton",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TidyIQ/gatsby-themes/issues"
  },
  "homepage": "https://github.com/TidyIQ/gatsby-themes/src/packages/gatsby-theme-tidyiq#readme",
  "eslintConfig": {
    "extends": "@tidyiq"
  },
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write build/**/*.{js,jsx,ts,tsx}",
