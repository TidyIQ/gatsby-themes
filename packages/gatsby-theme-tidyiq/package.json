{
  "name": "@tidyiq/gatsby-theme-tidyiq",
  "version": "0.0.1",
  "description": "Default Gatsby theme",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TidyIQ/gatsby-themes.git"
  },
  "keywords": [
    "gatsby"
  ],
  "author": "Jake Clayton",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TidyIQ/gatsby-themes/issues"
  },
  "homepage": "https://github.com/TidyIQ/gatsby-themes/src/packages/gatsby-theme-tidyiq#readme",
  "eslintConfig": {
    "extends": "@tidyiq"
  },
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write build/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "type-check": "tsc --noEmit",
    "build": "npm run lint:fix && rimraf ./src && tsc"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "@tidyiq/components": "*",
    "gatsby-plugin-netlify-cms": "^4.1.21",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-typescript": "^2.1.11",
    "netlify-cms-app": "^2.9.7",
    "react-helmet": "^5.2.1"
  },
  "peerDependencies": {
    "gatsby": "^2.15.28",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@tidyiq/eslint-config": "^2.3.1",
    "eslint": "^6.5.1",
    "gatsby": "^2.15.28",
    "prettier": "^1.18.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3"
  }
}
