{
  "name": "simple-site",
  "version": "0.0.1",
  "description": "Standard site using gatsby-theme-tidyiq package only",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TidyIQ/gatsby-simple-site.git"
  },
  "author": "Jake Clayton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TidyIQ/gatsby-simple-site/issues"
  },
  "homepage": "https://github.com/TidyIQ/gatsby-simple-site#readme",
  "eslintConfig": {
    "extends": "@tidyiq"
  },
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write **/*.{js,jsx,ts,tsx}",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "type-check": "tsc",
    "dev": "gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@tidyiq/components": "*",
    "@tidyiq/gatsby-theme-tidyiq": "*",
    "gatsby": "^2.15.28",
    "react": "^16.10.1",
    "react-dom": "^16.10.1"
  },
  "devDependencies": {
    "@tidyiq/eslint-config": "^2.3.1",
    "eslint": "^6.5.1",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  }
}
